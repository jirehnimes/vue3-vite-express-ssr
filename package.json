{
  "name": "vue3-vite-express-ssr",
  "author": {
    "name": "Jireh Nimes",
    "email": "jjnimes@gmail.com"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start:server": "ts-node --esm --project tsconfig.server.json src/server/index.ts",
    "dev:client": "vite",
    "dev:server": "nodemon --config nodemon.server.json",
    "build": "npm run build:client && npm run build:server",
    "build:client": "run-p type-check:client build-only:client",
    "build:server": "run-p type-check:server build-only:server",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only:client": "vite build --outDir dist/client",
    "build-only:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "type-check:client": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:server": "tsc --noEmit -p tsconfig.server.json",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "express": "^4.18.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  }
}
